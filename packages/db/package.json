{
  "name": "@ashgw/db",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "execute": "prisma db execute",
    "format": "prettier . --write",
    "format:schema": "prisma format",
    "generate": "prisma generate",
    "lint": "tsc --noEmit && eslint .",
    "migrate:deploy": "prisma migrate deploy",
    "migrate:dev": "prisma migrate dev",
    "migrate:diff": "prisma migrate diff",
    "migrate:reset": "prisma migrate reset",
    "migrate:resolve": "prisma migrate resolve",
    "migrate:status": "prisma migrate status",
    "postinstall": "prisma generate",
    "pull": "prisma db pull",
    "push": "prisma db push",
    "seed": "prisma db seed",
    "start": "docker compose -f compose.yml up -d",
    "stop": "docker compose -f compose.yml down -v",
    "studio": "prisma studio",
    "validate": "prisma validate"
  },
  "dependencies": {
    "@ashgw/env": "workspace:*",
    "@ashgw/logger": "workspace:*",
    "@prisma/adapter-pg": "catalog:prisma6",
    "@prisma/client": "catalog:prisma6",
    "bufferutil": "4.0.9",
    "prisma": "catalog:prisma6",
    "prisma-markdown": "2.0.0",
    "server-only": "0.0.1",
    "ws": "8.18.1",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@ashgw/eslint-config": "workspace:*",
    "@ashgw/tsconfig": "workspace:*",
    "@types/ws": "^8.18.1",
    "ts-roids": "catalog:",
    "typescript": "catalog:"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./raw": {
      "types": "./src/raw.ts",
      "import": "./src/raw.ts"
    }
  },
  "main": "./dist/index.js",
  "type": "module",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "raw": [
        "src/raw.ts"
      ],
      "*": [
        "src/index.ts"
      ]
    }
  }
}
