{
  "name": "@ashgw/seeder",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "format": "prettier . --write",
    "lint": "tsc --noEmit && eslint .",
    "seed": "pnpm --filter @ashgw/db migrate:reset --force && tsx src/index.ts"
  },
  "dependencies": {
    "@ashgw/db": "workspace:*",
    "@ashgw/env": "workspace:*",
    "@ashgw/logger": "workspace:*",
    "@ashgw/security": "workspace:*",
    "@ashgw/ts-env": "catalog:",
    "aws-sdk": "2.1692.0"
  },
  "devDependencies": {
    "@ashgw/eslint-config": "workspace:*",
    "@ashgw/tsconfig": "workspace:*",
    "ts-roids": "catalog:",
    "tsx": "^4.19.4",
    "typescript": "catalog:"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    }
  },
  "type": "module",
  "typesVersions": {
    "*": {
      "*": [
        "src/index.ts"
      ]
    }
  }
}
