{
  "name": "@ashgw/components",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "format": "prettier . --write",
    "lint": "eslint",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@ashgw/analytics": "workspace:*",
    "@ashgw/constants": "workspace:*",
    "@ashgw/design": "workspace:*",
    "@ashgw/env": "workspace:*",
    "@ashgw/logger": "workspace:*",
    "@ashgw/monitor": "workspace:*"
  },
  "devDependencies": {
    "@ashgw/eslint-config": "workspace:*",
    "@ashgw/prettier-config": "workspace:*",
    "@ashgw/tsconfig": "workspace:*",
    "@types/react": "catalog:react18",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "react": "catalog:react18",
    "ts-roids": "catalog:",
    "typescript": "catalog:"
  },
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "default": "./src/index.ts"
    },
    "./*": {
      "types": "./dist/src/*.d.ts",
      "default": [
        "./src/*.ts",
        "./src/*.tsx"
      ]
    }
  },
  "license": "MIT",
  "peerDependencies": {
    "react": "catalog:react18",
    "zod": "3.23.8"
  },
  "type": "module"
}
